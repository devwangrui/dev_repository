.title_txt {
	font-size: 26px;
}

.gs_txt {
	font-size: 15px;
	word-break: break-all;
	min-height: 200px;
	margin-top: 5px;
}

.fh_group {
	padding: 0;
	margin: 0;
	width: 40px;
	float: left;
}

.fh_group .btn {
	width: 40px;
	height: 40px;
	float: left;
	margin-bottom: -1px;
	font-size: 18px;
}

.sj_group {
	width: 400px;
	min-height: 500px;
	max-height: 500px;
	float: left;
	border: 1px solid #adadad;
	border-radius: 3px;
	margin-left: 0px;
	overflow-y: scroll;
}

.sj_group::-webkit-scrollbar {
	display: none
}

.search_box {
	width: 100%;
	height: 35px;
	line-height: 35px;
}

.search_box .search_ipt {
	width: 398px;
	font-size: 18px;
	padding-left: 10px;
	position: absolute;
}

.search_box input::-webkit-input-placeholder {
	/* WebKit browsers */
	color: #999;
	font-size: 16px;
}

.search_box input:-moz-placeholder {
	/* Mozilla Firefox 4 to 18 */
	color: #999;
	font-size: 16px;
}

.search_box input::-moz-placeholder {
	/* Mozilla Firefox 19+ */
	color: #999;
	font-size: 16px;
}

.search_box input:-ms-input-placeholder {
	/* Internet Explorer 10+ */
	color: #999;
	font-size: 16px;
}

.data_ul {
	list-style: square;
	margin-top: 2px;
}

.data_li {
	height: 40px;
	width: 100%;
	border-bottom: 1px solid #adadad;
	background-color: rgba(255, 255, 255, .3);
	cursor: pointer;
	font-size: 10px;
}

.data_li:nth-child(2n+0) {
	background-color: #f9f9f9;
	font-size: 10px;
}

.data_li:hover {
	background-color: #eaf4f2;
	font-size: 10px;
}

.data_li .data_nameBox,
.data_li .data_idBox {
	height: 40px;
	float: left;
	font-size: 10px;
	line-height: 40px;
	background-color: rgba(0, 0, 0, .0);
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.data_li .data_nameBox {
	width: 45%;
	padding-left: 10px;
	font-size: 10px;
}

.data_li .data_idBox {
	width: 55%;
	font-size: 10px;
}

.disabled {
	visibility: hidden;
}