.modal3_body1_2 p,
.modal3_body1_3 p {
	font-size: 18px;
	color: #333333;
}

.modal3_body1_2 ul {
	list-style: none;
}

.modal3_body1_2_ul1 li,
.modal3_body1_2_ul2 li {
	float: left;
	width: 80px;
	height: 40px;
	margin: auto auto 10px 20px;
	font-size: 16px;
	line-height: 40px;
	text-align: center;
	cursor: pointer;
}

.modal3_body1_2_ul1 li:hover,
.modal3_body1_2_ul2 li:hover {
	border: 1px solid rgb(52, 122, 184);
	/*color: #FFFFFF;*/
	border-radius: 5px;
}

.modal3_body1_2_ul-select{
	background-color: rgb(52, 122, 184);
	color: #FFFFFF;
	border-radius: 5px;
}

.modal3_body1_2_imgul {
	margin-top: 10px;
}

.modal3_body1_2_imgul li {
	float: left;
	width: 154px;
	height: 100px;
	margin-left: 10px;
	margin-bottom: 5px;
	border-radius: 4px;
	border: 1px solid #C1CDCD;
	overflow: hidden;
	cursor: pointer;
}

.modal3_body1_2_imgul li img {
	width: 151px;
	height: 97px;
}

.modal3_body1_2_imgul li:hover {
	border-radius: 4px;
	border: 1px solid rgb(52, 122, 184);
}

.modal3_body1_2_imgul-select{
	border-radius: 4px;
	border: 1px solid rgb(52, 122, 184) !important;
}

.modal3_body1_2_imgul_bt li {
	width: 124px;
	height: auto;
}	

.modal3_body1_2_imgul_bt li img {
	width: 120px;
	height: auto;
}
.modal3_body1_2_imgul_dt li {
	width: 184px;
	height: auto;
}

.modal3_body1_2_imgul_dt li img {
	width: 180px;
	height: auto;
}

.modal3_body1_2_imgul_ybp li {
	width: 124px;
	height: auto;
}

.modal3_body1_2_imgul_ybp li img {
	width: 120px;
	height: auto;
}

.modal3_body1_3 {
	padding-top: 20px;
	padding-left: 20px;
}

